<jmad-model-definition name="SPS">
  <optics>
    <optic name="default optics" overlay="false">
      <init-files>
        <call-file path="strength/lhc_newwp_2009.str" parse="STRENGTHS"/>
      </init-files>
    </optic>
  </optics>
  <default-optic ref-name="default optics"/>
  <sequences>
    <sequence name="sps">
      <ranges>
        <range name="ALL">
          <madx-range first="#s" last="#e"/>
          <twiss-initial-conditions name="default-twiss">
            <chrom value="true"/>
            <closed-orbit value="false"/>
            <centre value="false"/>
          </twiss-initial-conditions>
        </range>
      </ranges>
      <default-range ref-name="ALL"/>
    </sequence>
  </sequences>
  <default-sequence ref-name="sps"/>
  <init-files>
    <call-file location="RESOURCE" path="init-options.madx"/>
    <call-file path="beams/LHC_beam_injection.beamx"/>
    <call-file path="elements/sps2009.ele"/>
    <call-file path="aperture/aperturedb_1_2009.dbx"/>
    <call-file path="aperture/aperturedb_2_2009.dbx"/>
    <call-file path="sequence/sps2009.seq"/>
    <call-file path="aperture/aperturedb_3_2009.dbx"/>
    <call-file path="strength/lhc_newwp_2009.str"/>
    <call-file path="strength/elements.str"/>
  </init-files>
  <path-offsets>
    <repository-offset value="sps/optics2009/SPS/SPSRing/2009"/>
    <resource-offset value="sps"/>
  </path-offsets>
  <svn-revision>$Revision$</svn-revision>
</jmad-model-definition>