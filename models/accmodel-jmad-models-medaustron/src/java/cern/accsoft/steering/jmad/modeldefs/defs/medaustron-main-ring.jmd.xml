<jmad-model-definition name="MedAustron Main Ring">
  <optics>
    <optic name="injection" overlay="false">
      <init-files>
        <call-file location="RESOURCE" path="mr/mr-inj.str" parse="STRENGTHS"/>
        <call-file location="RESOURCE" path="mr/mr-corrector.str" parse="STRENGTHS"/>
      </init-files>
    </optic>
    <optic name="extraction" overlay="false">
      <init-files>
        <call-file location="RESOURCE" path="mr/mr-extr.str" parse="STRENGTHS"/>
        <call-file location="RESOURCE" path="mr/mr-corrector.str" parse="STRENGTHS"/>
      </init-files>
    </optic>
  </optics>
  <default-optic ref-name="injection"/>
  <sequences>
    <sequence name="mr">
      <beam/>
      <ranges>
        <range name="ALL">
          <madx-range first="#s" last="#e"/>
          <twiss-initial-conditions name="unnamed twiss">
            <chrom value="true"/>
            <closed-orbit value="false"/>
            <centre value="false"/>
          </twiss-initial-conditions>
        </range>
      </ranges>
      <default-range ref-name="ALL"/>
    </sequence>
  </sequences>
  <default-sequence ref-name="mr"/>
  <init-files>
    <call-file location="RESOURCE" path="common/elements.def"/>
    <call-file location="RESOURCE" path="mr/mr.def"/>
    <call-file location="RESOURCE" path="mr/mr.seq"/>
  </init-files>
  <path-offsets>
    <resource-offset value="ma"/>
  </path-offsets>
  <svn-revision>$Revision$</svn-revision>
</jmad-model-definition>