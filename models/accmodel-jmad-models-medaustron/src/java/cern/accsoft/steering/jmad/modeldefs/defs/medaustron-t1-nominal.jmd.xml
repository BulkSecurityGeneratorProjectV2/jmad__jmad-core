<jmad-model-definition name="MedAustron T1 (nominal)">
  <optics>
    <optic name="nominal" overlay="false">
      <init-files>
        <call-file location="RESOURCE" path="t1/t1-rf1.str" parse="STRENGTHS"/>
        <call-file location="RESOURCE" path="t1/t1-corrector.str" parse="STRENGTHS"/>
      </init-files>
    </optic>
    <optic name="RF2" overlay="false">
      <init-files>
        <call-file location="RESOURCE" path="t1/t1-rf2.str" parse="STRENGTHS"/>
        <call-file location="RESOURCE" path="t1/t1-corrector.str" parse="STRENGTHS"/>
      </init-files>
    </optic>
    <optic name="Alt1" overlay="false">
      <init-files>
        <call-file location="RESOURCE" path="t1/t1-rf1_alt1.str" parse="STRENGTHS"/>
        <call-file location="RESOURCE" path="t1/t1-corrector.str" parse="STRENGTHS"/>
      </init-files>
    </optic>
    <optic name="Alt2" overlay="false">
      <init-files>
        <call-file location="RESOURCE" path="t1/t1-rf1_alt2.str" parse="STRENGTHS"/>
        <call-file location="RESOURCE" path="t1/t1-corrector.str" parse="STRENGTHS"/>
      </init-files>
    </optic>
    <optic name="Alt3" overlay="false">
      <init-files>
        <call-file location="RESOURCE" path="t1/t1-rf1_alt3.str" parse="STRENGTHS"/>
        <call-file location="RESOURCE" path="t1/t1-corrector.str" parse="STRENGTHS"/>
      </init-files>
    </optic>
  </optics>
  <default-optic ref-name="nominal"/>
  <sequences>
    <sequence name="t1">
      <beam/>
      <ranges>
        <range name="ALL">
          <madx-range first="#s" last="#e"/>
          <twiss-initial-conditions name="unnamed twiss">
            <chrom value="true"/>
            <closed-orbit value="false"/>
            <centre value="false"/>
            <betx value="3.0"/>
            <bety value="3.0"/>
            <alfx value="0.0"/>
            <alfy value="0.0"/>
            <dx value="0.0"/>
            <dy value="0.0"/>
            <dpx value="0.0"/>
            <dpy value="0.0"/>
          </twiss-initial-conditions>
        </range>
      </ranges>
      <default-range ref-name="ALL"/>
    </sequence>
  </sequences>
  <default-sequence ref-name="t1"/>
  <init-files>
    <call-file location="RESOURCE" path="common/elements.def"/>
    <call-file location="RESOURCE" path="t1/t1.def"/>
    <call-file location="RESOURCE" path="t1/t1.seq"/>
  </init-files>
  <path-offsets>
    <resource-offset value="ma"/>
  </path-offsets>
  <svn-revision>$Revision$</svn-revision>
</jmad-model-definition>