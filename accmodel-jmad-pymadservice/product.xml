<?xml version="1.0" encoding="UTF-8"?>
<products>
	<product name="accmodel-jmad-pymadservice" version="0.0.1" directory="accmodel/jmad/accmodel-jmad-pymadservice">
		<jar>accmodel-jmad-gui.jar</jar>
		<dependencies>
			<dep product="log4j"/>
			<dep product="spring"/>
			<dep product="cglib2"/>
			<dep product="accmodel-jmad-core"/>
			<dep product="accmodel-jmad-models-lsa"/>
			<dep product="accmodel-jmad-models-default"/>
			<dep product="accmodel-jmad-models-ptc"/>
			<dep product="accmodel-jmad-models-medaustron"/>
			<dep product="accsoft-steering-commons"/>

			<!-- for testing -->
			<dep product="junit" local="true"/>
		</dependencies>
		<application name="jmad-gui" title="Simple gui for madx">
			<initial-heap-size>40m</initial-heap-size>
			<max-heap-size>240m</max-heap-size>
			<main-class name="cern.accsoft.steering.jmad.gui.JMad"/>
		</application>
		<application name="yasp-file-browser" title="Quick browser for yasp-files">
			<initial-heap-size>40m</initial-heap-size>
			<max-heap-size>100m</max-heap-size>
			<main-class name="cern.accsoft.steering.util.meas.yasp.browse.YaspFileBrowser"/>
		</application>
	</product>

	
</products>
